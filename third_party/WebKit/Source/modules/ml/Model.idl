interface Model {
  [CallWith=ScriptState] Promise<long> addOperand(OperandOptions options);
  [CallWith=ScriptState] Promise<long> setOperandValue(unsigned long index, [AllowShared] ArrayBufferView data);
  [CallWith=ScriptState] Promise<long> addOperation(long type, sequence<unsigned long> inputs, sequence<unsigned long> outputs);
  [CallWith=ScriptState] Promise<long> identifyInputsAndOutputs(sequence<unsigned long> inputs, sequence<unsigned long> outputs);
  [CallWith=ScriptState] Promise<long> finish();
  [CallWith=ScriptState] Promise<Compilation> createCompilation();
};